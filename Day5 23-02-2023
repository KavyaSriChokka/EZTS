#Approach-1
s=input()
v="aeiouAEIOU"
n=len(s)
c=0
for i in s:
    if i in v:
        c+=1
x=n-c
print(str(c)+" "+str(x))

#Approach-2
s=input()
v="aeiouAEIOU"
n=len(s)
c=0
x=0
for i in s:
    if i in v:
        c+=1
    elif i.isalpha():
        x+=1
print(str(c)+" "+str(x))

