#PROGRAM7
#Print the count of word,vowel,cosonant
t=int(input())
for i in range(t):
    s=list(input().split())
    w=len(s)
    v="aeiouAEIOU"
    c=0
    x=0
    for i in s:
        for j in i:
            if j in v:
              c+=1
            else:
              x+=1
    print(str(w)+" "+str(c)+" "+str(x))

#PROGRAM8
t=int(input())
for i in range(t):
    a,b=input().split()
    r=""
    for i in b:
        if i not in a:
            r=r+i
    print(r)

#PROGRAM9
#Factorial
class cse:
    def factorial(self,n):
        r=1
        for i in range(1,n+1):
            r=r*i
        print(r)
ob=cse()
ob.factorial(5)

class a:
    def __init__(self,n):
        self.n=n
        print(n)
    def factorial(self):
        r=1
        for i in range(1,self.n+1):
            r=r*i
        print(r)
        
ob=a(5)
ob.factorial()
'''
#Factorial using Recursion
class a:
    def __init__(self,n):
        self.n=n
        print(n)
    def rfactorial(self):
        print(self.rfactorial(self.n))
    def factorial(self,n):
       
        if self.n==0 or self.n==1:
            return 1
        else:
            return n*self.factorial(n-1)
    
ob=a(6)
ob.rfactorial()

        
        
    















        

    
